      "express-mongo-sanitize": "^2.2.0",
      "express-rate-limit": "^6.7.0",
      "firebase-admin": "^11.11.0",
      "formidable": "^3.5.0",
      "google-auth-library": "^9.0.0",
      "googleapis": "^126.0.1",
      "google-libphonenumber": "^3.2.33",
      "helmet": "^7.0.0",
      "html-entities": "^2.4.0",
      "http-status": "^1.6.2",
      "image-size": "^1.0.2",
