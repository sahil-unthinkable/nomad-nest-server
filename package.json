{
    "name": "nomad-nest-server",
    "version": "1.0.0",
    "private": true,
    "repository": "https://github.com/sahil-unthinkable/nomad-nest-server.git",
    "scripts": {
      "start-old": "npm run masters-seed && pm2 start ecosystem.config.json",
      "start": "npm run masters-seed && node src/index.js",
      "dev": "npm run masters-seed && node src/index.js",
      "test": "jest -i --colors --verbose --detectOpenHandles",
      "test:watch": "jest -i --watchAll",
      "coverage": "jest -i --coverage",
      "coverage:coveralls": "jest -i --coverage --coverageReporters=text-lcov | coveralls",
      "lint": "eslint .",
      "lint:fix": "eslint . --fix",
      "prettier": "prettier --check **/*.js",
      "prettier:fix": "prettier --write **/*.js",
      "prepare": "husky install",
      "masters-seed": "cross-env node seed-script/index.js",
      "postinstall": "patch-package"
    },
    "keywords": [
      "node",
      "node.js",
      "boilerplate",
      "generator",
      "express",
      "rest",
      "api",
      "mongodb",
      "mongoose",
      "es6",
      "es7",
      "es8",
      "es9",
      "jest",
      "travis",
      "docker",
      "passport",
      "joi",
      "eslint",
      "prettier"
      ],
    "dependencies": {
      "@google-cloud/storage": "^6.12.0",
      "@sendgrid/mail": "^7.7.0",
      "axios": "^1.4.0",
      "bcryptjs": "^2.4.3",
